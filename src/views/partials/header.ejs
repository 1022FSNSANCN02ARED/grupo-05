<header class="container-header">
  <div class="container-burger">
    <img
    onclick="document.body.classList.toggle('open')"
    class="burger"
    src="/images/burger.png"
    />
  </div>
  
  <div class="container-logo">
    <a href="/"></a><img src="/images/logo.png" class="logo"/> 
  </div>
  <div class="menu-items">
    <div class="container-buscador">
      <input placeholder="Busca tu juego..." class="buscador" />
      <button class="boton-lupa"><i class="uil uil-search"></i></button>
    </div>
    <div class="menu-left">
      <div class="ingreso"><a href="#" class="menu-a">Ingresar</a></div>
      <div class="registro"><a href="#" class="menu-a">Registrarse</a></div> 
    </div>
  </div>
  <div class="container-carrito">
    <span class="badge"></span>
    <a href="#"></a><img src="/images/carrito.png" class="carrito"/> 
  </div>
</header>